<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.artistaapp">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:networkSecurityConfig="@xml/network_security_config">

        <activity android:name=".login.LoginActivity">

        </activity>
        <activity android:name=".login.RegisterActivity"></activity>

        <activity android:name=".home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".appointment.AppointmentActivity"></activity>
        <activity android:name=".love.LoveActivity"></activity>
        <activity android:name=".profile.ProfileActivity"></activity>
        <activity android:name=".profile.AccountSettingsActivity"></activity>
        <activity android:name=".booking.BookingActivity"/>
        <activity android:name=".booking.BookingDialogFragment"/>
    </application>
</manifest>
